{
  "expo": {
    "name": "Oober",
    "slug": "oober",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#183e5f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.jackleslie.oober"
    },
    "description": "React Native Uber Client",
    "githubUrl": "https://github.com/jackleslie/oober",
    "android": {
      "package": "com.jackleslie.oober"
    },
    "scheme": "oober"
  }
}
